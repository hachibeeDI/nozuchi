{
  "$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "space",
		"indentWidth": 2,
		"lineEnding": "lf",
		"lineWidth": 140,
		"attributePosition": "auto"
	},
	"organizeImports": { "enabled": true },

  "linter": {
    "enabled": true,
    "rules": { "recommended": true }
  }
}
